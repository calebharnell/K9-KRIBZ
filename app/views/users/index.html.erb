<h1>User List</h1>

<table class="table table-striped table-hover ">

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %> <%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.suburb %></td>
        <td><%= user.state %></td>
        <td><%= link_to 'Show Profile', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
