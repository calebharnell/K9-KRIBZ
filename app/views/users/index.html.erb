<div class="container">

  <h1>User List</h1><br>

  <table class="table table-striped table-hover ">

    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td></td>
        <td><%= user.first_name %></td>
        <td><%= user.suburb %>, <%= user.state %></td>
        <td><%= link_to 'Show Profile', user, class: "btn btn-primary btn-sm" %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>
